{
  "realm": "nomecliente",
  "enabled": true,
  "displayName": "nomecliente",
  "registrationAllowed": false,
  "registrationEmailAsUsername": false,
  "rememberMe": true,
  "verifyEmail": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "accessTokenLifespan": 300,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Standard authenticated user",
        "composite": false
      },
      {
        "name": "admin",
        "description": "Administrator — full access",
        "composite": false
      },
      {
        "name": "editor",
        "description": "Editor — create and update resources",
        "composite": false
      }
    ]
  },
  "clients": [
    {
      "clientId": "nomecliente-client",
      "name": "nomecliente Backend Client",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "implicitFlowEnabled": false,
      "redirectUris": [
        "http://localhost:4200/*",
        "http://localhost:3000/*",
        "http://localhost:8080/*",
        "https://localhost:8443/*"
      ],
      "webOrigins": ["*"],
      "defaultClientScopes": [
        "openid",
        "profile",
        "email",
        "roles",
        "web-origins"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access",
        "microprofile-jwt"
      ]
    }
  ],
  "users": [
    {
      "username": "developer",
      "enabled": true,
      "email": "developer@example.com",
      "firstName": "Dev",
      "lastName": "User",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "dev123",
          "temporary": false
        }
      ],
      "realmRoles": ["user", "admin"]
    },
    {
      "username": "editor",
      "enabled": true,
      "email": "editor@example.com",
      "firstName": "Editor",
      "lastName": "User",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "editor123",
          "temporary": false
        }
      ],
      "realmRoles": ["user", "editor"]
    },
    {
      "username": "testuser",
      "enabled": true,
      "email": "testuser@example.com",
      "firstName": "Test",
      "lastName": "User",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "test123",
          "temporary": false
        }
      ],
      "realmRoles": ["user"]
    }
  ]
}

